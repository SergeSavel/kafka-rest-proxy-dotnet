{
  "BasicAuthUsers": {
    "SergeSavel": {
      "Password": "password"
    }
  },
  "Kafka": {
    "Producer": {
      "MessageMaxBytes": 16777216,
      "CompressionType": "zstd"
    },
    "SecurityProtocol": "Plaintext"
  },
  "SchemaRegistry": {
    "Url": "http://localhost:8081"
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "http://localhost:5000"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "SergeSavel.KafkaRestProxy.Consumer": "Debug"
    }
  },
  "AllowedHosts": "localhost"
}
