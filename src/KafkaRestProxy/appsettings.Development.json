{
  "Kafka": {
    "ClientId": "kafka-proxy-dotnet",
    "BootstrapServers": "rn-kafka01.samberi.com:9092,rn-kafka02.samberi.com:9092,rn-kafka03.samberi.com:9092",
    "SaslMechanism": "plain",
    "SecurityProtocol": "SaslPlaintext",
    "Acks": "All"
  },
  "KafkaProducer": {
    "Partitioner": "ConsistentRandom",
    "CompressionType": "zstd",
    "LingerMs": 1
  },
  "KafkaConsumer": {
    "EnableAutoCommit": false,
    "AutoOffsetReset": "Earliest",
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
